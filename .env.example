# Mistral API Configuration
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_OCR_MODEL=mistral-ocr-latest

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/fra_ocr_db

# Redis Configuration
